                                                                                                      owl:onDatatype xsd:double ;
                                                                                                      owl:withRestrictions ( [ xsd:maxInclusive "1.42117977E8"^^xsd:double
                                                                                                                             ]
                                                                                                                           )
                                                                                                    ]
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :Semimajor_Axis ;
                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                               owl:onDatatype xsd:double ;
                                                                               owl:withRestrictions ( [ xsd:minInclusive "2.0495E8"^^xsd:double
                                                                                                      ]
