                                                 rr:termType  rr:IRI
                                               ] ;
                                 rr:predicate  :Has_Orbit_Center
                               ] ;
        rr:subjectMap          [ a            rr:TermMap , rr:SubjectMap ;
                                 rr:class     :Natural_Satellite ;
